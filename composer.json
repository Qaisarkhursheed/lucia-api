{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "ext-simplexml": "*",
        "aws/aws-sdk-php": "^3.208",
        "doctrine/dbal": "^3.1",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "^2.12",
        "google/apiclient-services": "^0.224.1",
        "illuminate/mail": "^8.55",
        "illuminate/notifications": "^8.55",
        "illuminate/redis": "^8.77",
        "irazasyed/larasupport": "^1.7",
        "laravel/lumen-framework": "^8.0",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "macsidigital/laravel-zoom": "^5.0",
        "mixpanel/mixpanel-php": "2.*",
        "predis/predis": "^2.0",
        "pusher/pusher-php-server": "^7.0",
        "pusher/pusher-push-notifications": "^1.1",
        "qferr/mjml-php": "^2.0",
        "rap2hpoutre/fast-excel": "^3.1",
        "skagarwal/google-places-api": "^2.0",
        "spatie/laravel-slack-alerts": "^1.2",
        "stripe/stripe-php": "^7.107",
        "symfony/lock": "^6.1",
        "symfony/semaphore": "^6.1",
        "tymon/jwt-auth": "^1.0",
        "yajra/laravel-datatables-oracle": "^9.18"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "flipbox/lumen-generator": "^8.2",
        "kitloong/laravel-migrations-generator": "^4.5",
        "mockery/mockery": "^1.3.1",
        "pelmered/fake-car": "^1.6",
        "phpunit/phpunit": "^9.3",
        "reliese/laravel": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/General.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
